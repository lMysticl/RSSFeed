<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sw.rssnews">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/abccom_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.sw.rssnews.MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity
            android:name="com.sw.rssnews.NewsDetail"
            android:label="News Details "
            android:parentActivityName="com.sw.rssnews.MainActivity"

            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sw.rssnews.MainActivity"
                />

        </activity>

    </application>

</manifest>